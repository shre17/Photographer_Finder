<div class="jumbotron">
  <h1 class="display-6">Membership Plan</h1>
  <%= link_to admin_articles_path, class: "btn btn-primary btn-md" do %>
    <i class="fa fa-arrow-left"></i> Back to list
  <% end %>
</div>

<div class="container">
  <div class="row">
    <div class="col-md-8 offset-md-2">
      <%= render 'form', membership_plan: @membership_plan %>
    </div>
  </div>
  <div class="row">
    <div class="col-md-8 offset-md-2 mt-2">
      <%= link_to 'Show', admin_membership_plan_path(@membership_plan) %> |
      <%= link_to 'Back', admin_membership_plans_path %>
    </div>
  </div>
</div>
