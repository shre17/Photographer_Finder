<div class="jumbotron">
  <h1 class="display-6">Edit Photographer</h1>
  <%= link_to admin_photographers_path, class: "btn btn-primary btn-md" do %>
    <i class="fa fa-arrow-left"></i> Back to list
  <% end %>
</div>

<div class="container">
  <div class="row">
    <div class="col-md-8 offset-md-2">
      <%= render 'form', photographer: @photographer %>
    </div>
  </div>
  <div class="row">
    <div class="col-md-8 offset-md-2 mt-2">
      <%= link_to 'Show', admin_photographer_path(@photographer) %> |
      <%= link_to 'Back', admin_photographers_path %>
    </div>
  </div>
</div>
