<%= form_for :upload_images, url: :upload_photographer_album_image_photographer_albums, html: { multipart: true, class: "dropzone"} do |f|  %>
  <%= f.hidden_field :album_id, value: @album.id %>
  <div class="fallback">
    <%= f.file_field :image %><br>
    <%= f.submit "Upload" %>
  </div>
<% end %>